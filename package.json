{
  "name": "yfiag-contracts",
  "version": "1.0.0",
  "description": "YAgnostic secure vaults",
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/YFIAG/contracts.git"
  },
  "scripts": {
    "compile": "npx truffle compile --all",
    "deploy:base": "npx truffle migrate --migrations_directory './migrations/base/'",
    "deploy:vault": "npx truffle migrate --migrations_directory './migrations/vault/'",
    "deploy:test_env": "npx truffle migrate --migrations_directory './migrations/test_env/'",
    "deploy:dao:test": "npx truffle migrate --migrations_directory './migrations/DAO/test/'",
    "deploy:dao:mainnet": "npx truffle migrate --migrations_directory './migrations/DAO/mainnet/'",
    "deploy:bridge:local": "npx truffle migrate --migrations_directory ./migrations/bridge_development",
    "deploy:bridge:eth-test": "npx truffle migrate --migrations_directory ./migrations/bridge_eth/testnet --network rinkeby",
    "deploy:bridge:bsc-test": "npx truffle migrate --migrations_directory ./migrations/bridge_bsc/testnet --network bscTestnet",
    "deploy:bridge:eth-main": "npx truffle migrate --migrations_directory ./migrations/bridge_eth/mainnet",
    "deploy:bridge:bsc-main": "npx truffle migrate --migrations_directory ./migrations/bridge_bsc/mainnet",
    "deploy:bsc-yfiag": "npx truffle migrate --migrations_directory ./migrations/bsc_yfiag",
    "deploy:strategy:bsc-venus":"npx truffle migrate --migrations_directory ./migrations/strategies/bsc-venus/",
    "deploy:strategy:comp-no-fold":"npx truffle migrate --migrations_directory ./migrations/strategies/comp-no-fold/",
    "deploy:strategy:crv-stable":"npx truffle migrate --migrations_directory ./migrations/strategies/crv-stable/",
    "deploy:strategy:crv-wrenbtc":"npx truffle migrate --migrations_directory ./migrations/strategies/crv-wrenbtc/",
    "deploy:strategy:crv-ycrv":"npx truffle migrate --migrations_directory ./migrations/strategies/crv-ycrv/",
    "dev:lint": "npx eslint . && npx solhint -f table contracts/**/*.sol",
    "dev:contract-size": "npx truffle run contract-size",
    "dev:coverage": "npx truffle run coverage",
    "docgen:dao": "npx solidity-docgen -i ./contracts/dao/ -o ./docs/dao -t ./doc_templates/ -H doc_templates/helpers.js  --solc-module ./node_modules/solc",
    "dev:docgen": "npx solidity-docgen -i ./contracts -o ./docs -t docgen  -H docgen/helpers.js --solc-module ./node_modules/solc",
    "dev:ganache": "npx ganache-cli -q --gasLimit 6721975 --gasPrice 20000000000 -e 10000000 -p 8545",
    "generate-abi": "npx truffle-abi -o ./abi",
    "generate-artifacts": "npm run compile && npm run generate-abi",
    "manage": "node ./scripts/manage_vault.js",
    "postinstall": "npm run generate-artifacts",
    "flatten": "node flatten.js ./contracts",
    "eth:fork": "ganache-cli --gasLimit 8000000 -f https://mainnet.infura.io/v3/b9ca234069684f689b206c8ad3f32524 -i 1 -e 10000 -p 8545 --chainId 1"
  },
  "dependencies": {
    "@openzeppelin/contracts": "^3.4.1",
    "@openzeppelin/upgrades": "^2.8.0",
    "@truffle/hdwallet-provider": "^1.2.1",
    "big-number": "^2.0.0",
    "bignumber.js": "^9.0.1",
    "colors": "^1.4.0",
    "dotenv": "^8.2.0",
    "solc": "^0.6.12",
    "solidity-docgen": "^0.5.11",
    "truffle": "^5.1.62",
    "web3": "^1.3.3",
    "web3-privatekey-provider": "^1.2.0",
    "web3-utils": "^1.3.1",
    "yargs": "^16.2.0"
  },
  "devDependencies": {
    "@nomiclabs/buidler": "^1.4.8",
    "@openzeppelin/test-helpers": "^0.5.10",
    "@resolver-engine/imports": "^0.3.3",
    "@resolver-engine/imports-fs": "^0.3.3",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "chai-bn": "^0.2.1",
    "cids": "^1.1.6",
    "eslint": "^7.18.0",
    "eslint-plugin-mocha": "^8.0.0",
    "ganache-cli": "^6.12.2",
    "ganache-time-traveler": "^1.0.15",
    "ipfs": "^0.54.2",
    "prettier-plugin-solidity": "^1.0.0-beta.3",
    "solhint": "^3.3.2",
    "solidity-coverage": "^0.7.14",
    "solidity-docgen": "^0.5.11",
    "truffle-assertions": "^0.9.2",
    "truffle-contract-size": "^2.0.1"
  }
}
